cd input
rm -f CMHEART.inp
rm -f Darcy_parameters.inp
ln -s ./2d_input/quad4/stab/5^2/CMHEART.inp
ln -s ./2d_input/quad4/stab/5^2/Darcy_parameters.inp
cd ../
bin/x86_64-linux/ConvergenceStudyExample-debug-gnu
mv output/cmgui.exnode input/2d_input/quad4/stab/5^2
mv output/cmgui.exelem input/2d_input/quad4/stab/5^2
mv output/Debug_Darcy.txt input/2d_input/quad4/stab/5^2
mv output/conv.node input/2d_input/quad4/stab/5^2


